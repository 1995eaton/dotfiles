out_to_console yes
out_to_x no
background no
update_interval 2
total_run_times 0
use_spacer none

TEXT
\
\
VOL[${execi 5 \
	if amixer sget Master | grep -o "\[off\]" > /dev/null; \
		then echo "MM"; \
	else amixer sget Master | grep -o "[0-9]\+%"; \
fi}] | \
\
BAT[${execi 10 \
	acpi | grep -o "\(\(Discharging\|Charging\|Full\|Unknown\), \)\?[0-9]\+%" | \
		sed -e "s/Unknown/Charging/g" | \
		sed -e "s/,/:/g"}] | \
\
CPU[${cpu cpu0}% ${cpu cpu2}% ${cpu cpu4}% ${cpu cpu6}%]\
[${execi 5 \
	sensors | grep "Core 1" | \
		grep -o "  \++.*C " | \
		sed -e 's/[+ ]//g' | \
		egrep -o "[0-9]+.[0-9]+"}C] | \
\
RAM[$mem/$memmax] | ${time %^a %m/%d/%y %I:%M%p}
