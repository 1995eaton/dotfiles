alt + {comma,period}
  bspc desktop -C {backward,forward}
~button1
  bspc pointer -g focus
alt + shift + {h,j,k,l}
  bspc window -f {left,down,up,right}
alt + !button{1-3}
  bspc pointer -t %i %i
alt + @button{1-3}
  bspc pointer -u
alt bracket{left,right}
  bspc window -f {prev,next}
alt + shift + bracket{left,right}
	bspc window -f {prev,next} && bspc window -t fullscreen
alt + ctrl + {1-9}
  bspc window -r 0.{1-9}
ctrl + shift + h
  bspc window -e right -20 || bspc window -e left -20
ctrl + shift + l
  bspc window -e right +20 || bspc window -e left +20
ctrl + shift + k
  bspc window -e up -20 || bspc window -e down -20
ctrl + shift + j
  bspc window -e up +20 || bspc window -e down +20
alt + {_,shift} + s
  {killall -9 conky bar; bspc config -m DVI-I-1 top_padding 0,bspc config -m DVI-I-1 top_padding 15; /home/jake/.config/bspwm/panel/init.sh}
alt + shift + b
  bspc config -m DVI-I-1 window_gap 15 && bspc config -m DVI-D-0 window_gap 15
alt + b
  bspc config -m DVI-I-1 window_gap 0 && bspc config -m DVI-D-0 window_gap 0
alt + button{1-3}
  bspc pointer -g {move,resize_side,resize_corner}
alt + shift + d
  bspc window -c
super + q
  killall conky bar; bspc quit
alt + {o,i}
  bspc window -t {floating,fullscreen}
alt + {_,shift + }{1-9,0}
  bspc {desktop -f,window -d} ^{1-9,10}
alt + p
  dmenu_run -nb \#1c1c1c -nf \#8f8f8f -sb \#f92672
alt + shift + w
  google-chrome-unstable --test-type --harmony_arrow_functions
alt + w
  /home/jake/chromium/src/out/Release/chrome --test-type --ppapi-flash-path=/opt/google/chrome-unstable/PepperFlash/libpepflashplayer.so
ctrl + alt + w
  chromium --test-type --ppapi-flash-path=/opt/google/chrome-unstable/PepperFlash/libpepflashplayer.so
alt + ctrl + w
  primusrun chromium --test-type --harmony_arrow_functions
alt + shift + n
  chromium --test-type --incognito --harmony_arrow_functions
alt + t
  urxvtc -geometry 120x40 -e tmux -2 -q
alt + m
  /home/jake/scripts/hide_cursor/hide.sh
alt + Escape
  pkill -USR1 -x sxhkd
alt + Down
  amixer -c 0 set Master 0+ toggle
super + x
  xrdb /home/jake/.Xresources
ctrl + alt + {j,l,k,h}
  /home/jake/scripts/move.sh {down,right,up,left}
@XF86MonBrightnessUp
   xbacklight -inc 5
@XF86MonBrightnessDown
   xbacklight -dec 5
@XF86AudioMute
  amixer -c 0 set Master 0+ toggle
@XF86AudioLowerVolume
  amixer -c 0 set Master 2-
@XF86AudioRaiseVolume
  amixer -c 0 set Master 2+
alt + grave
  bspc window next -t fullscreen=on -f
alt + shift + Return
  bspwm_tile.zsh
