#!/bin/sh

if [ $EUID -ne 0 ]; then
  echo "It's a weird tree."
else
  cat << "EOF"
       @@@                      @@@
      @///@     /@@            @;  @
     @/  //@   /  ;@          @;;  @
    @//  ///@  @  ;;@        @;;;;;@
    @///////@  @;;;;;@@     @;;;;@@
    @///////@   @@;;;;;@  @@;;;;@
     @/////@      @/;;;;@@;;;;;@
      @///@   @@@   @;;;;;;;;;@
       @@@@  @///@   /@;;;;;@@
        @;@ @/  //@    @;;;@
    @@@ @;;@//  ///@   @;;;@
   @///@ @;@///////@ @@;@;;@@
  @/  //@@;@///////@@;;;@;;@;@@    @@@
 @//  ///@;;@/////@@ ; ;/;;;;;;@  @///@
 @///////@ ; @///@@ ; ; ;;;;;;;@ @/  //@
 @///////@; ;;@@@ @; ; ;;;;;;;;;@//  ///@
  @/////@; ; @     @;;;;;;;;@;;;@///////@
   @///@ @@;;@     @;;@;;;;;;;;;@///////@
    @@@    @;;@    @;;;;;;;;;;;;@@/////@   @@@
           @;;@    @;;;;;;   @;;@ @///@   @///@
           @;;;@   @;@    ;;;;;;@  @@@@  @/  //@
            @;;;@@@;;;;;;;;;;;;;@   @;@ @//  ///@
             @;;;;@;;;;;;;;;;;;@    @;@@@///////@
              @;;;@;;;;;;;;;;;;@   @ ; ;@///////@
               @;@;; ;;;;;;;;;;@   @; ;;;@/////@
               @;@;  ;;;;;;;;;@@@@@;;@@@;;@///@
                @;; ;;;;;; ;;;;;;;;;@///@@ @@@
                @;  ;;;;;  ;;;;;;@@@/  //@
               @;; ;;;;;; ;;;@@@@ @//  ///@
               @;;;;;;;;;;;;;@    @///////@
              @;;;;;;  ;;;;;@     @///////@
              @;;;;;   ;;;;@       @/////@
             @;;;;;;  ;;;;;@        @///@
             @;;;;;;;;;;;;@          @@@
             @;;;;;;;;;;;;@
            @;;; ;;;;;;;;@
          /@@;;   ;;;;;;;@
         /;;@;;   ;;;;;;;@
        /  ;;;;   ;;;;;;;@
        @  ;@;;; ;;;;;@;;;@
        @;;;@;;;;;;;;@ ;;;@
         @;;@;;;;;;;;@;;;;;@
         @@;;@;;;;;;;;@;;;;;@@@@@
      @@@;;@;;@@;;;;;;;@@@;;;; ; @@
     @  ;;;;;;;;@@@@@@@   @;;;; ; ;@
    @;;;;;;;;;@@           @;;;;;;;@
     @;;;;;;@@              @@;;;;;@
      @@@@@@                  @@@@@
EOF
fi
