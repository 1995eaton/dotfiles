out_to_console yes
out_to_x no
background no
update_interval 2
total_run_times 0
use_spacer none

TEXT
\
\
\l ${execi 0.05 monitor.sh} \
\c${time %A \f2|\f\ %B %d, %Y \f2|\f\%k:%M:%S %p} \
\rVOL[\f1${execi 5 \
	if amixer sget Master | grep -o "\[off\]" > /dev/null; \
		then echo "MM"; \
	else amixer sget Master | grep -o "[0-9]\+%"; \
fi}\f\] | \
\
CPU[\f1${cpu cpu0}% ${cpu cpu1}% ${cpu cpu2}% ${cpu cpu3}%\f\]\
[\f1${execi 5 \
	sensors | grep "Core 1" | \
		grep -o "  \++.*C " | \
		sed -e 's/[+ ]//g' | \
		egrep -o "[0-9]+.[0-9]+"}C\f\] | \
\
RAM[\f1$mem\f\/\f1$memmax\f\] 
